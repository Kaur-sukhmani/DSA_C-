// fibonnaci series
#include<stdio.h>
int main(){
    int n,c;
    int a = 0;
    int b =1;
    printf("enter the number till what you want ");
    scanf("%d",&n);
    printf("\n %d",a);
    printf("\n %d",b);
    for(int i = 2;i<n;i++){
        c=a+b;
        printf("\n %d",c);
        a=b;
        b=c;
    }
    return 0;
}
