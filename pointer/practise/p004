// Character Array
#include<iostream>
using namespace std;
int main(){
    int arr[5] = {1,2,3,4,5};
    char ch[6] = "abcde";

    cout<<arr<<endl;
    // pehli loc ka address print hota hain 

    cout<<ch<<endl;
    // puri ki puri string hi print ho gyi

    char *c=&ch[0];  
    // prints entire string 
    cout<<c<<endl;

    char temp='z';
    char *p=&temp;

    cout<<p<<endl;
    return 0;
}
